{
    "files.autoSave": "afterDelay",
    "window.zoomLevel": 0,
    "window.enableMenuBarMnemonics": false,
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "workbench.iconTheme": "vscode-icons",
    "workbench.sideBar.location": "right",
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "editor.renderIndentGuides": false,
    "editor.lineNumbers": "relative",
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "workbench.activityBar.visible": true,
    "workbench.statusBar.visible": true,
    "window.menuBarVisibility": "hidden",
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.quickOpen.closeOnFocusLost": true,
    "workbench.editor.enablePreview": false,
    "zenMode.hideStatusBar": false,
    "zenMode.hideTabs": false,
    "zenMode.hideActivityBar": false,
    "editor.codeLens": false,
    "workbench.startupEditor": "none",
    "omnisharp.projectLoadTimeout": 240,
    "explorer.autoReveal": false,
    "git.autorefresh": false,
    "vsicons.projectDetection.disableDetect": true,
    "git.decorations.enabled": false,
    "workbench.panel.location": "bottom",
    //MsSql Settings
    "mssql.messagesDefaultOpen": false,
    "mssql.connections": [
        {
            "server": "ALK-ELIXIR",
            "database": "",
            "authenticationType": "Integrated",
            "profileName": "ELIXIR",
            "password": ""
        },
        {
            "server": ".",
            "database": "",
            "authenticationType": "Integrated",
            "profileName": "LOCALHOST",
            "password": ""
        }
    ],
    //Vim Settings
    "vim.leader": "<space>",
    "vim.hlsearch": true,
    "vim.easymotion": true,
    "vim.incsearch": false,
    "vim.useSystemClipboard": true,
    "vim.statusBarColorControl": false,
    "vim.visualstar": true,
    "vim.cmdLineInitialColon": true,
    "vim.statusBarColors": {
        "normal": "#005f5f",
        "insert": "#5f0000",
        "visual": "#5f00af",
        "visualline": "#005f87",
        "visualblock": "#86592d",
        "replace": "#000000"
    },
    "vim.insertModeKeyBindings": [
        {
            "before": [ //Exit Insert Mode
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.otherModesKeyBindings": [
        {
            "before": [ //Format Block
                "<C-i>"
            ],
            "after": [
                "=",
                "i",
                "{"
            ]
        },
        {
            "before": [ //Find
                "<C-f>"
            ],
            "after": [
                "*"
            ]
        }
    ],
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "before": [ //No Highlighting
                "leader",
                "/"
            ],
            "after": [],
            "commands": [
                {
                    "command": ":nohl",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Close Tab Group
                "leader",
                "c",
                "t"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.closeEditorsInGroup",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Close All Editors
                "leader",
                "c",
                "a"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.closeAllEditors",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Ctrlp
                "leader",
                "c",
                "p"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.quickOpen"
                }
            ]
        },
        {
            "before": [ //Ctrlp Buffers
                "leader",
                "c",
                "b"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.files.action.focusOpenEditorsView"
                }
            ]
        },
        {
            "before": [ //goto previous
                "leader",
                "g",
                "p"
            ],
            "after": [
                "<C-o>"
            ]
        },
        {
            "before": [ //Save and Exit
                "Z",
                "Z"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.closeWindow",
                    "args": []
                }
            ]
        },
        {
            "before": [ //New Line Normal
                "leader",
                "o"
            ],
            "after": [
                "o",
                "<Esc>"
            ]
        },
        {
            "before": [ //New Line Normal Above
                "leader",
                "O"
            ],
            "after": [
                "O",
                "<Esc>"
            ]
        },
        {
            "before": [ //Insert Space Normal
                "leader",
                "i"
            ],
            "after": [
                "i",
                " ",
                "<Esc>"
            ]
        },
        {
            "before": [ //Insert Space Line Feed Normal
                "leader",
                "I"
            ],
            "after": [
                "i",
                "<CR>",
                "<Esc>",
                ">",
                ">"
            ],
            "commands": [
                {
                    "command": "editor.action.trimTrailingWhitespace",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Goto definition
                "leader",
                "g",
                "d"
            ],
            "after": [
                "g",
                "d"
            ]
        },
        {
            "before": [ //Toggle Layout
                "leader",
                "l",
                "o"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.toggleEditorGroupLayout",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Build
                "leader",
                "b"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.tasks.build",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Trim Trailing Whitespace
                "leader",
                "w",
                "s"
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.trimTrailingWhitespace",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Find Usages
                "leader",
                "f",
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.referenceSearch.trigger",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Joint Tab Groups
                "leader",
                "j",
                "g"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.joinTwoGroups",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Toggle Panel Closed
                "leader",
                "c",
                "c"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.togglePanel",
                    "args": []
                },
                {
                    "command": "closeReferenceSearch",
                    "args": [],
                    "when": "referenceSearchVisible && !config.editor.stablePeek"
                }
            ]
        },
        {
            "before": [ //Ctrlp Open Problems
                "leader",
                "c",
                "o"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.actions.view.problems",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Goto Implementations
                "leader",
                "g",
                "i"
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.goToImplementation",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Show Debug Console
                "leader",
                "c",
                "d"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.debug.action.toggleRepl",
                    "args": [],
                    "when": "!inDebugMode"
                }
            ]
        },
        {
            "before": [ //New File
                "leader",
                "n"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.files.newUntitledFile",
                    "args": []
                }
            ]
        },
        {
            "before": [ //Format Document
                "leader",
                "f",
                "d"
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.formatDocument",
                    "args": [],
                    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
                }
            ]
        },
        {
            "before": [ //Format Document
                "leader",
                "r",
                "q"
            ],
            "after": [],
            "commands": [
                {
                    "command": "extension.runQuery",
                    "args": [],
                    "when": "editorTextFocus && editorLangId == 'sql'"
                }
            ]
        },
        {
            "before": [ //Advanced New File
                "leader",
                "a",
                "n"
            ],
            "after": [],
            "commands": [
                {
                    "command": "extension.advancedNewFile",
                    "args": [],
                    "when": "editorTextFocus"
                }
            ]
        },
        {
            "before": [ //Terminal Focus
                "leader",
                "t"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.terminal.focus",
                    "args": [],
                    "when": "editorTextFocus"
                }
            ]
        },
        {
            "before": [ //Fold Area - WIP
                "leader",
                "z",
                "f"
            ],
            "commands": [
                {
                    "command": "editor.fold",
                    "args": [],
                    "when": "editorTextFocus"
                }
            ]
        },
        {
            "before": [ //UnFold Area
                "leader",
                "z",
                "a"
            ],
            "commands": [
                {
                    "command": "editor.unfoldAll",
                    "args": [],
                    "when": "editorTextFocus"
                }
            ]
        },
        {
            "before": [ //Reload Window
                "leader",
                "r",
                "l"
            ],
            "commands": [
                {
                    "command": "workbench.action.reloadWindow",
                    "args": [],
                    "when": "editorTextFocus"
                }
            ]
        },
        {
            "before": [ //ReplaceWithRegister - inner word
                "g", "r", "i", "w"
            ],
            "after": [
                "v", "i", "w", "\"", "*", "p", "y", "i", "w"
            ]
        },
        {
            "before": [ //ReplaceWithRegister - inner Word
                "g", "r", "i", "W"
            ],
            "after": [
                "v", "i", "W", "\"", "*", "p", "y", "i", "W"
            ]
        },
        {
            "before": [ //ReplaceWithRegister - around word
                "g", "r", "a", "w"
            ],
            "after": [
                "v", "a", "w", "\"", "*", "p", "y", "a", "w"
            ]
        },
        {
            "before": [ //ReplaceWithRegister - line
                "g", "r", "r"
            ],
            "after": [
                "V", "\"", "*", "p", "Y"
            ]
        },
        {
            "before": [ //ReplaceWithRegister - end of line
                "g", "r", "$"
            ],
            "after": [
                "v", "$", "\"", "*", "p", "Y"
            ]
        },
        {
            "before": [ //Split Window
                "leader",
                "s",
                "p"
            ],
            "commands": [
                {
                    "command": "workbench.action.splitEditor",
                    "args": [],
                    "when": "editorTextFocus"
                }
            ]
        }
    ]
}